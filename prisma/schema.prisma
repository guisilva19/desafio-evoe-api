generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model user {
  id            String  @id @default(uuid())
  nome          String
  email         String  @unique
  senha         String
  telefone      String
  link          String
  administrador Boolean @default(false)

  criado_em     DateTime @default(now())
  atualizado_em DateTime @updatedAt
}
